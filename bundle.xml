<?xml version="1.0"?>
<bundle>
  <executables>
    <executable name="Meld">meldlauncher</executable>
  </executables>
  <libraries>
    <library>libgtk-3.0.dylib</library>
    <library>libgirepository-1.0.1.dylib</library>
    <library>libgtksourceview-4.0.dylib</library>
    <library>libreadline.8.dylib</library>
    <library>gdk-pixbuf-2.0/2.10.0/loaders/*.so</library>
    <library>gtk-3.0/3.0.0/immodules/*.so</library>
    <library>gtk-3.0/3.0.0/printbackends/*.so</library>
  </libraries>
  <resources>
    <resource target_dir="Frameworks">lib/gtk-3.0/3.0.0/immodules.cache</resource>
    <resource target_dir="Frameworks">lib/gdk-pixbuf-2.0/2.10.0/loaders.cache</resource>
    <resource>share/glib-2.0</resource>
  </resources>
  <frameworks>
    <framework>tmp/Python.framework</framework>
  </frameworks>
  <icon>
  </icon>
  <locales>
    <!-- should go in Resources/locale, be able to access mo files and also add them to plist -->
  </locales>
  <gtk3>
    <!-- special module to take care of gtk specifis: pixbuf loader, input modules, regenerating typelib from gir, theme/icon ... -->
    <css>
      <!-- making changes to css file -->
    </css>
  </gtk3>
  <gtk4>
  </gtk4>
  <fontconfig/>  <!-- for setting up etc and configs and stuff -->
</bundle>
